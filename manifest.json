{
    "manifest_version": 2,
    "name": "Wordreference it!",
    "description": "Searches Wordreference.com for a translation.",
    "version": "1.0",
    "homepage_url": "https://github.com/keirua/firefox-wordreference-it",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "icons": {
      "64": "icons/wr-icon64.png"
    },
    "permissions": [
        "activeTab",
        "contextMenus"
    ]
}
